{
  "name": "@christianesk/github-calendar",
  "version": "1.4.0",
  "description": "Gets Github calendar data in format SVG by user and year.",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "source": "src/index.js",
  "scripts": {
    "test": "jest",
    "prebuild": "npm test",
    "build": "microbundle",
    "prepublish": "npm run build",
    "publish": "npm run prepublish"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "Github",
    "Github Calendar",
    "Calendar",
    "Github contributions",
    "Github calendar contributions",
    "Calendar contributions",
    "contributions"
  ],
  "author": "Christian Mena <christian.91.gm@hotmail.com> (https://stkngesk.tech)",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "microbundle": "^0.13.0",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "*"
  }
}
